int main() {
    int n;
    cin >> n;
    
    string s;
    for (int i = 0; i < n; i++) {
        cin >> s;
        string oddS;
        string evenS;
        for (int j = 0; j < s.size(); j++) {
            if (j % 2 == 1) oddS += s[j];
            else evenS += s[j];
        }
        cout << evenS << " " << oddS << endl;
    }
    
    return 0;
}
