class Student {
    public:
    int score1[5];
    
    void input() {
        for (int i = 0; i < 5; i++) cin >> score1[i];
    }
    
    int calculateTotalScore() {
        int score2 = 0;
        for (int i = 0; i < 5; i++) {
            score2 = score2 + score1[i];
        }
        return score2;
    }
};
