void plusMinus(vector<int> arr) {
    float count1 = 0, count2 = 0, count3 = 0;
    float n = arr.size();
    for (int i = 0; i < n; i++) {
        if (arr[i] > 0) count1++;
        if (arr[i] < 0) count2++;
        if (arr[i] == 0) count3++;
    }
    cout << (float)(count1 / n) << setprecision(6) << fixed << endl
         << (float)(count2 / n) << setprecision(6) << fixed << endl
         << (float)(count3 / n) << setprecision(6) << fixed;
}
