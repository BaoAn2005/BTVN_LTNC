int main() { 
    int i = 4;
    
    double d = 4.0;
    string s = "HackerRank ";
   
    int a;
    double b;
    string s1;
    
    cin >> a >> b;
    cin.ignore();
    getline(cin, s1);
    
    cout << a + i << endl
         << setprecision(1) << fixed << b + d << endl
         << s + s1 << endl;
    
    return 0;
}
